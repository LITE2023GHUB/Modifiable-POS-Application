//Function//////////////////////////////////////////////////////////////////
    public void ItemCost(){
        double sum = 0;
        
        for(int i =0; i < jTable1.getRowCount(); i++){
            sum = sum + Double.parseDouble(jTable1.getValueAt(i, 2).toString());
        }
        SubTotal.setText(Double.toString(sum));
        double CostTotal = Double.parseDouble(SubTotal.getText());
        
        double CostTax = (CostTotal * 0.12)/100;
        String TaxTotal = String.format("₱ % .2f",CostTax);
        Tax.setText(TaxTotal);
        
        String iSubTotal = String.format("₱ % .2f",CostTotal);
        SubTotal.setText(iSubTotal);      
       
        String iTotal = String.format("₱ % .2f",CostTotal + CostTax);
        Total.setText(iTotal);
        
        String iBarcode = String.format("Total is % .2f",CostTax);
        BarCode.setText(iBarcode);
    }
    
    public void Change(){
        double sum = 0;
        double tax = 0.12;
        double cash = Double.parseDouble(Display.getText());
        
        for(int i =0; i < jTable1.getRowCount(); i++){
            sum = sum + Double.parseDouble(jTable1.getValueAt(i, 2).toString());
        }
        
        double CostTax = (sum * 0.12)/ 100;
        double cChange = (cash - (sum + CostTax));
        
        String GivenChange = String.format("₱ % .2f", cChange);
        Change.setText(GivenChange);
    }

