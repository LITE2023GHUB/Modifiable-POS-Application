    }                                             

    private void ButtonResetActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        DefaultTableModel model =(DefaultTableModel) jTable1.getModel();
        model.setRowCount(0);
        Change.setText("");
        Tax.setText("");
        Total.setText("");
        SubTotal.setText("");
        Display.setText("");
        BarCode.setText("");
    }                                           

    private void ButtonRemoveActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
        DefaultTableModel model =(DefaultTableModel) jTable1.getModel();

        int RemoveItem = jTable1.getSelectedRow();
        if(RemoveItem >= 0){
            model.removeRow(RemoveItem);
        }

        ItemCost();

        if (PaymentMethod.getSelectedItem().equals("Cash")){
            Change();
        }

        else{
            Change.setText("");
            Display.setText("");
        }
    }                                            

