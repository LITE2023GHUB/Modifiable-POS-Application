    private void ButtonPayActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        if (PaymentMethod.getSelectedItem().equals("Cash")){
            Change();
        }
        
        else{
            Change.setText("");
            Display.setText("");
        }
    }                                         

