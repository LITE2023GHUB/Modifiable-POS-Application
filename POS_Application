    private void ButtonPrintActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        MessageFormat header = new MessageFormat("Printing in progress");
        MessageFormat footer = new MessageFormat("Page {0, number, integer}");
        
        PrinterJob job = PrinterJob.getPrinterJob();
        job.setJobName("Receipt Printing");
        
        Printable printable = new Printable(){
            public int print(Graphics graphics, PageFormat pageFormat, int PageIndex) throws PrinterException{
                if (PageIndex > 0){
                    return NO_SUCH_PAGE;
                }
        Graphics2D g2d = (Graphics2D) graphics;
        g2d.translate(pageFormat.getImageableX(), pageFormat.getImageableY());
        
        String modeOfPayment = "Cash";
        String amountPaid = Display.getText();
        String changeReceived = Change.getText();
        
        JTable  table = jTable1;
        TableModel model = table.getModel();
        
        int tableStartX = 50;
        int tableStartY = 50;
        
        for(int row = 0; row < model.getRowCount(); row++){
            for(int col = 0; col < model.getColumnCount(); col++){
                Object value = model.getValueAt(row, col);
                g2d.drawString(value.toString(), tableStartX + col * 100, tableStartY + row * 20);
            }
        }
        
        int infoStartY = tableStartY + (model.getRowCount() + 1) *20;
        
        g2d.drawString("Mode Of Payment: " + modeOfPayment, tableStartX, infoStartY);
        g2d.drawString("SubTotal: " + SubTotal.getText(), tableStartX, infoStartY + 20);
        g2d.drawString("Amount Paid: ₱" + amountPaid, tableStartX, infoStartY + 40);
        g2d.drawString("Change Received: " + changeReceived,tableStartX, infoStartY + 60);
      
        return PAGE_EXISTS;
        }
        };
        
        job.setPrintable(printable);
        
        try{
            job.printDialog();
            job.print();
        }
        
        catch(java.awt.print.PrinterException e){
          System.err.format("No Printer found", e.getMessage());
        }

    }                                           

    private JFrame frame;
    
    private void ButtonExitActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        frame = new JFrame("Exit");
        if(JOptionPane.showConfirmDialog(frame, "Confirm if you want to exit", "Point Of Sale", JOptionPane.YES_NO_OPTION) == JOptionPane.YES_NO_OPTION){
            System.exit(0);
        }
    }                                          

